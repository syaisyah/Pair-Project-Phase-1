<!--/cars/rent'   disini adalah untuk  bar menampilkan list cars yang sedang di sewa-->
<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>List Rent Cars</title>
</head>

<body>
  <h1>List Rent Cars</h1>
  <table border="1">
    <tr>
      <th>ID</th>
      <th>Image</th>
      <th>Merk</th>
      <th>Type</th>
      <th>Category</th>
      <th>Released Year</th>
      <th>Rental Price/Day</th>
      <th>Status</th>
    </tr>

   <% data.forEach( el => { %>
      <tr>
        <td><%= el.id %></td>
        <td>
          <img src="<%= el.imageURL %>">
        </td>
        <td><%= el.merk %></td>
        <td><%= el.type %></td>
        <td><%= el.category %></td>
        <td><%= el.released_year %></td>
        <td><%= el.rupiah %></td>
        <td><%= el.status %></td>
      </tr>
    <% }) %>



</body>

</html>